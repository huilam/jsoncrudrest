#
crud.poiapi.restapi.baseurl=/apps/poi
crud.poiapi.dbconfig=jdbc.postgres-poi
crud.poiapi.tablename=poi
crud.poiapi.jsonattr.PoiID.colname=poiid
crud.poiapi.jsonattr.PoiUID.colname=poiuid
crud.poiapi.jsonattr.PoiName.colname=poiname
crud.poiapi.jsonattr.Attributes.sql=select key, value from poiattribute where poiid = {PoiID}
crud.poiapi.jsonattr.Attributes.mapping={"key":"value"}
crud.poiapi.jsonattr._PoiImage.sql=SELECT poi.poiid, poi.poiuid, img.imagetype, face.faceid, img.image imagebase64 FROM poi, image img, face WHERE face.isdefault = 'Y' and face.imageid = img.imageid AND poi.poiid = face.poiid AND poi.poiuid = {PoiUID} 
crud.poiapi.jsonattr._RepoCategories.sql=select rep.repositoryid, rep.repositoryname, cat.categoryid, cat.categoryname, cat.matchingthreshold from poicategory poicat, category cat, repository rep where poicat.categoryid = cat.categoryid and rep.repositoryid = cat.repositoryid and poicat.poiid = {PoiID}
#Repository[RepositoryID, RepositoryName, RepositoryID, CategoryList:[CategoryID, CategoryName, MatchingThreshold ]]
crud.poiapi.jsonattr._PoiOtherImages.sql=select poi.poiid, face.faceid, face.imageid, image.image, image.imagetype from poi, face, image where face.isdefault <> 'Y' and face.poiid = poi.poiid and poi.poiid = {PoiID}
#TotalFaces
#FaceList[FaceID, ImageType, Image, ImageThumbnail]
#crud.poiapi.exception.on-unknown-jsonattr=true
crud.poiapi.restapi.fetch.limit=100
crud.poiapi.restapi.id=PoiID
crud.poiapi.restapi.echo.jsonattr.prefix=echo.
crud.poiapi.restapi.plugin.implementation=hl.jsoncrudrest.plugins.PoiCrudServicePlugin


#
crud.jsoncrud_cfg.restapi.baseurl=/v1/appconfig
crud.jsoncrud_cfg.dbconfig=jdbc.postgres
crud.jsoncrud_cfg.tablename=jsoncrud_cfg
crud.jsoncrud_cfg.jsonattr.cfgId.colname=cfg_id
crud.jsoncrud_cfg.jsonattr.appNamespace.colname=cfg_app_namespace
crud.jsoncrud_cfg.jsonattr.moduleCode.colname=cfg_module_code
crud.jsoncrud_cfg.jsonattr.createdTimestamp.colname=created_timestamp
crud.jsoncrud_cfg.jsonattr.enabled.colname=enabled
crud.jsoncrud_cfg.jsonattr.kvpair.sql=select cfg_key, cfg_value from jsoncrud_cfg_values where cfg_id = {cfgId} 
crud.jsoncrud_cfg.jsonattr.kvpair.mapping={"cfg_key":"cfg_value"}
crud.jsoncrud_cfg.jsonattr.kvpair.insert.sql=insert into jsoncrud_cfg_values (cfg_id, cfg_key, cfg_value) values ({cfgId}, {cfg_key}, {cfg_value})
crud.jsoncrud_cfg.jsonattr.keys.sql=select cfg_key from jsoncrud_cfg_values where cfg_id = {cfgId} 
crud.jsoncrud_cfg.jsonattr.keys.mapping=["cfg_key"]
crud.jsoncrud_cfg.jsonattr.emptyresult.sql=select cfg_key from jsoncrud_cfg_values where 1=2 
#crud.jsoncrud_cfg.exception.on-unknown-jsonattr=true
#-
crud.jsoncrud_cfg.restapi.fetch.limit=1000
crud.jsoncrud_cfg.restapi.id=cfgId
crud.jsoncrud_cfg.restapi.echo.jsonattr.prefix=echo.
crud.jsoncrud_cfg.restapi.plugin.implementation=hl.jsoncrudrest.plugins.CRUDServiceSysOutPlugin

